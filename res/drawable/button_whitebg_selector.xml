<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:state_enabled="true" android:shape="rectangle">
            <solid android:color="@color/gray_f5_t9" />
            <stroke android:width="2.0dip" android:color="@color/gray_f5_t9" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent" />
            <stroke android:width="2.0dip" android:color="@color/transparent" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent" />
            <stroke android:width="2.0dip" android:color="@color/transparent" />
        </shape>
    </item>
</selector>